export TF_VAR_tenancy_ocid= <id da tenancy>
export TF_VAR_user_ocid= <id do usuario>
export TF_VAR_compartment_ocid= <id do compartment>

export TF_VAR_fingerprint=$(cat ~/.oci/oci_api_key_fingerprint)
export TF_VAR_private_key_path=~/.oci/oci_api_key.pem

export TF_VAR_ssh_public_key=$(cat ~/.ssh/id_rsa.pub)
export TF_VAR_ssh_private_key=$(cat ~/.ssh/id_rsa)

export TF_VAR_region= <id da regiao>